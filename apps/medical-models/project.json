{
  "name": "medical-models",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/medical-models",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project medical-models --web",
  "targets": {
    "deploy": {
      "command": "node ./apps/medical-models/upload-to-s3.js"
    },
    "dockerbuild": {
      "command": "docker build . -t jonathonclifford/mm-ui"
    },
    "dockerpush": {
      "command": "docker push jonathonclifford/mm-ui"
    },
    "prettier": {
      "command": "prettier -w --ignore-path ./apps/medical-models/.prettierignore ./apps/medical-models"
    },
    "prettier-check": {
      "command": "prettier -c --ignore-path ./apps/medical-models/.prettierignore ./apps/medical-models"
    }
  }
}
