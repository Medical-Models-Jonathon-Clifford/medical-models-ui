{
  "name": "hello-wasm",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/hello-wasm",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project hello-wasm --web",
  "targets": {
    "build": {
      "dependsOn": [{ "dependencies": true, "target": "build" }],
      "executor": "nx:run-commands",
      "options": {
        "command": "webpack --config apps/hello-wasm/webpack.config.js"
      }
    },
    "serve": {
      "dependsOn": [{ "target": "build" }],
      "executor": "nx:run-commands",
      "options": {
        "command": "webpack serve --config apps/hello-wasm/webpack.config.js --open"
      }
    }
  }
}
